{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express.js 백엔드 서버로 Google Gemini API를 통한 다중 이미지 분석 기능을 제공합니다.",
  "main": "dist/app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:contract": "jest tests/contract",
    "test:integration": "jest tests/integration",
    "test:unit": "jest tests/unit",
    "lint": "echo 'ESLint will be configured in T004'",
    "clean": "rm -rf dist coverage",
    "prestart": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.3",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@google/genai": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "winston": "^3.17.0"
  }
}
